{
  "$schema": "http://json-schema.org/draft-07/hyper-schema#",
  "$id": "https://raw.githubusercontent.com/johnbillion/wp-json-schemas/trunk/schemas/rest-api/properties/object-links.json",
  "title": "WP_REST_API_Object_Links",
  "description": "The relations for the object and its properties.",
  "type": "object",
  "additionalProperties": {
    "type": "array",
    "items": {
      "type": "object",
      "required": [
        "href"
      ],
      "properties": {
        "href": {
          "type": "string",
          "format": "uri"
        },
        "embeddable": {
          "type": "boolean"
        }
      }
    }
  }
}
